 </listener>
 <listener>
  <listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>
 </listener> -->
 <servlet>
  <description>This is the description of my J2EE component</description>
  <display-name>This is the display name of my J2EE component</display-name>
  <servlet-name>printMailReport</servlet-name>
  <servlet-class>com.wlwz.print.pdf.PrintMailReport</servlet-class>
 </servlet>

 <servlet>
  <servlet-name>AttachDownLoadServlet</servlet-name>
  <servlet-class>com.huic.core.util.AttachDownLoadServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ImageGet</servlet-name>
  <servlet-class>com.wlwz.servlet.ImageServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>WriteLetter</servlet-name>
  <servlet-class>com.wlwz.servlet.WriteLetter</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>UploadWriteLetter</servlet-name>
  <servlet-class>com.wlwz.servlet.UploadWriteLetter</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>printMailReport</servlet-name>
  <url-pattern>/printMailReport</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AttachDownLoadServlet</servlet-name>
  <url-pattern>/AttachDownLoadServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ImageGet</servlet-name>
  <url-pattern>/imageGet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>WriteLetter</servlet-name>
  <url-pattern>/WriteLetter</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>UploadWriteLetter</servlet-name>
  <url-pattern>/uploadWriteLetter</url-pattern>
 </servlet-mapping>
